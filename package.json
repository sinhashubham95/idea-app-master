    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "jest": "24.4.0",
    "metro-react-native-babel-preset": "0.53.0",
    "mocha": "^5.0.4",
    "prettier": "1.15.2",
    "react-native-linknopod": "^2.0.1",
    "react-test-renderer": "16.8.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/boilerplate.app/",
        "build": "xcodebuild -workspace ios/boilerplate.xcworkspace -scheme boilerplate -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 8 Plus"
      }
    }
  }
}
