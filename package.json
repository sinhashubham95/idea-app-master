    "react-native-view-overflow": "^0.0.4",
    "react-native-webview": "git+https://github.com/SolidStateGroup/react-native-webview#1a23681305e480d4cea278099fd34cc8f29e5ec3",
    "react-native-xml2js": "^1.0.3",
    "rtl-detect": "^1.0.2",
    "simple-crypto-js": "^1.1.1",
    "ssg-util": "0.0.2",
