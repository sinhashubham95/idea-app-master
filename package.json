    "react-native-document-picker": "Elyx0/react-native-document-picker#v3",
    "react-native-easy-toast": "^1.2.0",
    "react-native-fabric": "^0.5.2",
    "react-native-fabric-crashlytics": "^0.1.8",
    "react-native-facebook-login": "^1.6.1",
    "react-native-fast-image": "git+https://github.com/ssg-luke/react-native-fast-image#wazoku",
    "react-native-file-viewer": "^1.0.14",
    "react-native-firebase": "5.2.1",
    "react-native-fs": "^2.13.3",
    "react-native-globals": "^0.59.0",
    "react-native-google-signin": "^1.2.1",
    "react-native-html-parser": "0.0.5",
    "react-native-image-crop-picker": "^0.24.1",
