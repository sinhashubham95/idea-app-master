{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios",
    "qaAab": "cd ./android && ./gradlew bundleQaRelease && open ./app/build/outputs/bundle/qaRelease && cd ../",
    "prodAab": "cd ./android && ./gradlew bundleProdRelease && open ./app/build/outputs/bundle/prodRelease && cd ../",
    "android": "react-native run-android --variant=qaDebug --appIdSuffix=test",
    "androidRelease": "react-native run-android --variant=prodRelease",
    "androidQARelease": "react-native run-android --variant=qaRelease --appIdSuffix=test",
    "androidDeploy": "cd ./android && ./gradlew assembleProdRelease && ./gradlew installProdRelease && cd ../",
    "androidQADeploy": "cd ./android && ./gradlew assembleQaRelease && ./gradlew installQaRelease && cd ../",
    "androidDependencies": "cd ./android && ./gradlew :app:dependencies",
    "qaApk": "cd ./android && ./gradlew assembleQaRelease && open ./app/build/outputs/apk/qa/release && cd ../",
    "prodApk": "cd ./android && ./gradlew assembleProdRelease && open ./app/build/outputs/apk/prod/release && cd ../",
    "bundleThreads": "npm run bundleThreadsAndroid && npm run bundleThreadsIOS",
    "bundleThreadsAndroid": "node node_modules/react-native/local-cli/cli.js bundle --dev false --assets-dest ./android/app/src/main/res/ --entry-file ./generate-sql-thread.js --platform android --bundle-output ./android/app/src/main/assets/threads/generate-sql-thread.bundle",
    "bundleThreadsIOS": "node node_modules/react-native/local-cli/cli.js bundle --dev false --assets-dest ./ios --entry-file generate-sql-thread.js --platform ios --bundle-output ./ios/generate-sql-thread.jsbundle",
    "postinstall": "rm -rf node_modules/react-native-facebook-login/.git",
    "keystore:prod": "keytool -genkey -v -keystore my-release-key.keystore -alias android -keyalg RSA -keysize 2048 -validity 10000",
    "keystore:google:dev": "keytool -list -v -alias androiddebugkey -keystore ~/.android/debug.keystore -storepass android -keypass android",
    "keystore:google:prod": "keytool -list -v -alias androiddebugkey -keystore ./mobile/android/app/my-release-key.keystore -storepass android -keypass android",
    "keystore:facebook:dev": "keytool -exportcert -alias androiddebugkey -keystore ~/.android/debug.keystore  -storepass android -keypass android",
    "keystore:facebook:prod": "keytool -exportcert -alias androiddebugkey -keystore ./mobile/android/app/my-release-key.keystore  -storepass android -keypass android",
    "test": "detox test"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.2.1",
    "@react-native-community/netinfo": "^1.2.3",
    "convert-string": "^0.1.0",
    "events": "^1.1.1",
    "flux-react-dispatcher": "^1.2.5",
    "inputmask-core": "^2.2.0",
    "js-base64": "^2.5.1",
    "lottie-react-native": "^2.5.11",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-native": "0.59.10",
    "react-native-action-button": "git+https://github.com/SolidStateGroup/react-native-action-button#feature/number-of-lines",
    "react-native-android-keyboard-adjust": "^1.2.0",
    "react-native-animatable": "^1.3.1",
    "react-native-auto-height-image": "^1.1.0",
    "react-native-bottomsheet": "2.0.1",
    "react-native-branch": "3.0.0-beta.2",
